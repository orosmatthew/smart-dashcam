generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  id        Int      @id @default(autoincrement())
  timestamp DateTime
  friendly  String?
  thumbnail String?
  url       String
  type      String
  public    Boolean  @default(false)
}

model IMU {
  id        Int      @id @default(autoincrement())
  timestamp DateTime @unique
  gyroX     Float
  gyroY     Float
  gyroZ     Float
  accelY    Float
  accelZ    Float
  accelX    Float
  magX      Float
  magY      Float
  magZ      Float
  temp      Float
}

model Bookmark {
  id        Int      @id @default(autoincrement())
  timestamp DateTime @unique
}
